{
  "name": "brunch-with-class",
  "version": "0.0.1",
  "main": "public/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "lodash": "^2.4.1",
    "backbone": "^1.1.2",
    "jquery": "^2.1.1",
    "chaplin": "^1.0.0",
    "underscore.string": "^2.3.3",
    "console-polyfill": "^0.1.1",
    "backbone.localStorage": "~1.1.9",
    "bootstrap": "~3.2.0",
    "backbone.collectionsubset": "~0.1.3",
    "velocity": "~0.10.1",
    "marked": "~0.3.2",
    "backbone.hammer": "~1.0.1"
  },
  "overrides": {
    "marked": {
      "main": "lib/marked.js"
    },
    "backbone.collectionsubset": {
      "main": "lib/backbone.collectionsubset.js"
    },
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "lodash": "*",
        "jquery": "*"
      }
    },
    "backbone.hammer": {
      "main": "./backbone.hammer.js"
    },
    "underscore.string": {
      "main": "lib/underscore.string.js",
      "dependencies": {
        "lodash": "*"
      }
    },
    "font-awesome": {
      "main": [
        "font",
        "css/font-awesome.css"
      ]
    },
    "velocity": {
      "main": [
        "jquery.velocity.js",
        "velocity.ui.js"
      ]
    }
  }
}
